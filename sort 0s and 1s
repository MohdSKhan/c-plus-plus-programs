
#include<bits/stdc++.h>
using namespace std;
#define lli long long int
#define ulli unsigned long long int
#define MOD 1000000007
void solve(int *arr,int n){
    int left=0;
    int i=0;
    while(i<n){
        if(arr[i]==0){
            swap(arr[i], arr[left]);
            left++; 
        }
        
        i++;
        
    }
}

int main()
{
            int n;
            cin>>n;
            int arr[n];
            for(int i=0;i<n;i++){
                cin>>arr[i];
            }
            solve(arr,n);

            for(int i=0;i<n;i++){
                cout<<arr[i]<<" ";
            }
            

}



