
#include<bits/stdc++.h>
using namespace std;
#define lli long long int
#define ulli unsigned long long int
#define MOD 1000000007


int main()
{
            
                int n;
                cin>>n;
                int arr[n];
                for(int i=0;i<n;i++){
                    cin>>arr[i];
                }

                int key; cin>>key;
                int i;
                for( i=0;i<n;i++){
                    if(arr[i]==key){
                        cout<<key<<" found at "<<i<<endl;
                        break;
                    }
                }

                if(i==n){
                    cout<<key<<" not found";
                }
                
}



