
#include<bits/stdc++.h>
using namespace std;
#define lli long long int
#define ulli unsigned long long int
#define MOD 1000000007

int fib(int n){
    if(n<=2){
        return n-1;
    }

    int a=0,b=1,sum;
    for(int i=1;i<=n-2;i++){
        sum= a+b;
        a=b;
        b=sum;
    }
    return sum;
}
int main()
{
            
                int n;
                cin>>n;
                cout<<fib(n)<<endl;
}



