
#include<bits/stdc++.h>
using namespace std;
#define lli long long int
#define ulli unsigned long long int
#define MOD 1000000007


int main()
{
            
                int n;
                cin>>n;
                int arr[n];
                for(int i=0;i<n;i++){
                    cin>>arr[i];
                }
                int maximum= INT_MIN;
                int minimum= INT_MAX;
                
                for(int i=0;i<n;i++){
                    if(arr[i]>maximum){
                        maximum= arr[i];
                    }

                    if(arr[i]<minimum){
                        minimum= arr[i];
                    }
                }

                cout<<"max is "<<maximum<<" and min is "<<minimum;

                
}



