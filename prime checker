
#include<bits/stdc++.h>
using namespace std;
#define lli long long int
#define ulli unsigned long long int
#define MOD 1000000007
bool prime(int n){
    if(n==1 or n==0){
        return false;
    }
    for(int i=2;i<n;i++){
        if(n%i==0){
            return false;
        }
    }
    return true;
}


int main()
{
            
                int n;
                cin>>n;
                
                cout<<prime(n)<<endl;
            

}



